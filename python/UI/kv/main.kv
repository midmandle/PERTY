<RootLayout>:
    BoxLayout:
        Button:
            text:"Add Task"
            on_press:
                root.manager.transition.direction = 'left'
                root.manager.current="addTask"
        Button:
            text:"New Workflow"
            on_press:
                root.manager.transition.direction = 'left'
                root.manager.current="newWorkflow"
        Button:
            text:"Add Task List"
            on_press:
                root.manager.transition.direction = 'left'
                root.manager.current="addTaskList"
